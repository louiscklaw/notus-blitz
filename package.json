{
  "name": "notus-blitz",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run dev",
    "build:tailwind": "tailwind build styles/index.css -o styles/tailwind.css"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@popperjs/core": "2.9.1",
    "@tailwindcss/forms": "0.2.1",
    "chart.js": "2.9.4",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "@prisma/client": "3.16.0-integration-tmp-revert-node14.1",
    "blitz": "0.45.5",
    "react-dom": "18.0.0",
    "react": "18.0.0",
    "zod": "3.17.10",
    "formik": "2.2.9",
    "tailwindcss": "2.0.4"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "prettier-plugin-prisma": "3.14.0",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.7",
    "prisma": "3.16.0-integration-tmp-revert-node14.1",
    "autoprefixer": "10.2.5",
    "postcss": "8.2.8"
  },
  "private": true
}
